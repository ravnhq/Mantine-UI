{
  "name": "@ravnhq/mantine-ui",
  "version": "0.0.1",
  "description": "A collection of reusable UI components built with Mantine for seamless integration in React projects.",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types.d.ts",
  "files": [
    "dist"
  ],
  "private": true,
  "scripts": {
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "tsc": "tsc",
    "lint:ts": "eslint --cache --ext ts,tsx,js,jsx .",
    "lint:ts:all": "eslint --cache --ext ts,tsx,js,jsx ./src",
    "lint:ts:fix:all": "eslint --fix --ext ts,tsx,js,jsx ./src",
    "lint:ts:fix": "npm run lint:ts --fix",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "static": "npm-run-all --parallel -c tsc lint:ts format:check",
    "rollup": "rollup -c --bundleConfigAsCjs && npm pack",
    "prepare": "husky"
  },
  "author": "Ravn",
  "license": "MIT",
  "devDependencies": {
    "@chromatic-com/storybook": "^1.6.0",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@ravnhq/eslint-config": "^2.0.0",
    "@ravnhq/prettier-config": "^1.1.0",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@storybook/addon-essentials": "^8.1.11",
    "@storybook/addon-interactions": "^8.1.11",
    "@storybook/addon-links": "^8.1.11",
    "@storybook/addon-onboarding": "^8.1.11",
    "@storybook/blocks": "^8.1.11",
    "@storybook/react": "^8.1.11",
    "@storybook/react-vite": "^8.1.11",
    "@storybook/test": "^8.1.11",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/node": "^20.14.9",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "eslint-plugin-storybook": "^0.8.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.38",
    "postcss-import": "^16.1.0",
    "postcss-preset-mantine": "^1.15.0",
    "postcss-scss": "^4.0.9",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rollup": "^4.18.0",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "sass": "^1.77.6",
    "storybook": "^8.1.11",
    "storybook-dark-mode": "^4.0.2",
    "typescript": "^5.5.2",
    "vite-tsconfig-paths": "^4.3.2",
    "@mantine/core": ">=7.11.0",
    "@mantine/hooks": ">=7.11.0"
  },
  "dependencies": {
    "@tabler/icons-react": "^3.7.0"
  },
  "peerDependencies": {
    "@mantine/core": ">=7.0.0",
    "@mantine/hooks": ">=7.0.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "@tabler/icons-react": ">=3.7.0"
  },
  "engines": {
    "node": ">=18.20.0"
  }
}
